# ==========================================
# AMBIENTE DE STAGING (HOMOLOGAÇÃO)
# ==========================================

# Database - Neon Staging Branch
DATABASE_URL="postgresql://user:password@ep-xxxxx.us-east-1.aws.neon.tech/caixinhas-staging?sslmode=require"

# NextAuth Configuration
NEXTAUTH_URL="https://caixinhas-staging.vercel.app"
NEXTAUTH_SECRET="staging-secret-different-from-production-32chars+"

# Google OAuth (Staging)
# Authorized redirect URIs: https://caixinhas-staging.vercel.app/api/auth/callback/google
GOOGLE_CLIENT_ID="your-staging-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your-staging-client-secret"

# Firebase Configuration (Projeto Staging)
NEXT_PUBLIC_FIREBASE_API_KEY="your-staging-firebase-api-key"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="caixinhas-staging.firebaseapp.com"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="caixinhas-staging"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="caixinhas-staging.appspot.com"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="123456789"
NEXT_PUBLIC_FIREBASE_APP_ID="1:123456789:web:abcdef"

# AWS S3 Configuration (Bucket Staging)
AWS_S3_BUCKET_NAME="caixinhas-staging-uploads"
AWS_S3_REGION="us-east-1"
AWS_ACCESS_KEY_ID="your-staging-access-key"
AWS_SECRET_ACCESS_KEY="your-staging-secret-key"

# Kiwify Webhook (Staging)
KIWIFY_WEBHOOK_SECRET="staging-kiwify-secret"

# Gemini AI
GOOGLE_API_KEY="your-gemini-api-key"

# Environment
NODE_ENV="production"
NEXT_PUBLIC_APP_ENV="staging"
