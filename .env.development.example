# ==========================================
# AMBIENTE DE DESENVOLVIMENTO LOCAL
# ==========================================

# Database - Neon Development Branch
# Crie uma branch 'development' no Neon Console
DATABASE_URL="postgresql://user:password@ep-xxxxx.us-east-1.aws.neon.tech/caixinhas-dev?sslmode=require"

# NextAuth Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="dev-secret-change-in-production-minimum-32-characters-long"

# Google OAuth (Desenvolvimento)
# Criar credenciais separadas em: https://console.cloud.google.com
# Authorized redirect URIs: http://localhost:3000/api/auth/callback/google
GOOGLE_CLIENT_ID="your-dev-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your-dev-client-secret"

# Firebase Configuration (Projeto Development)
# Criar projeto separado em: https://console.firebase.google.com
NEXT_PUBLIC_FIREBASE_API_KEY="your-dev-firebase-api-key"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="caixinhas-dev.firebaseapp.com"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="caixinhas-dev"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="caixinhas-dev.appspot.com"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="123456789"
NEXT_PUBLIC_FIREBASE_APP_ID="1:123456789:web:abcdef"

# AWS S3 Configuration (Bucket Development)
# Criar bucket separado: caixinhas-dev-uploads
AWS_S3_BUCKET_NAME="caixinhas-dev-uploads"
AWS_S3_REGION="us-east-1"
AWS_ACCESS_KEY_ID="your-dev-access-key"
AWS_SECRET_ACCESS_KEY="your-dev-secret-key"

# Kiwify Webhook (Desenvolvimento)
# Usar webhook de teste do Kiwify
KIWIFY_WEBHOOK_SECRET="dev-kiwify-secret"

# Gemini AI (Desenvolvimento)
# Pode usar a mesma key ou uma separada
GOOGLE_API_KEY="your-gemini-api-key"

# Environment
NODE_ENV="development"
NEXT_PUBLIC_APP_ENV="development"

# Debug (opcional)
DEBUG="prisma:*"
LOG_LEVEL="debug"
